
@{
    ViewData["Title"] = "ContactSubmissions";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@model List<ContactFormSubmission>
<div class="app-main__outer">
    <div class="app-main__inner">
        <div class="row">
            <div class="col-lg-12">
                <div class="main-card mb-3 card">
                    <div class="card-body">
                        <h5 class="card-title">Kategoriyalar</h5>
                        <table class="mb-0 table table-hover">
                            <thead>
                                <tr>
                                   
                                    <th>Ad</th>
                                    <th>Telefon</th>
                                    <th>Email</th>
                                    <th>Mesaj</th>
                                    <th>Tarix</th>
                                    <th class="text-center" colspan="2">
                                        <a href="/Admin/AddUser">
                                            <i class="pe-7s-plus"></i>
                                        </a>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>


                                @foreach (var submission in Model)
                                {
                                    <tr>
                                        <td>@submission.ContactName</td>
                                        <td>@submission.ContactPhone</td>
                                        <td>@submission.ContactEmail</td>
                                        <td>@submission.ContactMessage</td>
                                        <td>@submission.ContactSubmissionDate</td>
                                    </tr>
                                }
                           

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


